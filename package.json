{
    "name": "bedrock-addon-icons",
    "displayName": "Bedrock Addon Icons",
    "description": "Bedrock Addon Icons is an extension that provides Godot icons inside of VSCode, for the purpose of creating Bedrock addons.",
    "version": "1.0.2",
    "publisher": "SirLich",
    "repository": {
        "type": "git",
        "url": "https://github.com/SirLich/bedrock-addon-icons"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "activationEvents": [],
    "icon": "images/icon.png",
    "categories": [
        "Themes"
    ],
    "main": "./extension.js",
    "contributes": {
        "iconThemes":[
            {
                "id": "bedrock-icons",
                "label": "Bedrock Icons",
                "path": "./theme.json"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.64.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "eslint": "^8.6.0",
        "glob": "^7.2.0",
        "mocha": "^9.1.3",
        "typescript": "^4.5.4",
        "@vscode/test-electron": "^2.0.3"
    }
}